{
    "bitwise": true,
    "browser": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "futurehostile": true,
    "latedef": true,
    "noarg": true,
    "nocomma": true,
    "nonbsp": true,
    "nonew": true,
    "predef": [
        "define",
        "Promise"
    ],
    "strict": "implied",
    "undef": true,
    "unused": "vars"
}
