{
    "extensions": {
        "services": [
            {
                "key": "dialogService",
                "implementation": "DialogService.js",
                "depends": [ "overlayService", "$q", "$log" ]
            },
            {
                "key": "overlayService",
                "implementation": "OverlayService.js",
                "depends": [ "$document", "$compile", "$rootScope" ]
            }
        ],
        "templates": [
            {
                "key": "overlay-dialog",
                "templateUrl": "templates/overlay.html"
            }
        ]
    }
}