{
    "extensions": {
        "constants": [
            {
                "key": "DEFAULT_AUTO_DISMISS",
                "value": 3000
            }
        ],
        "controllers": [
            {
                "key": "MessageController",
                "implementation": "MessageController.js",
                "depends": ["$scope"]
            }
        ],
        "services": [
            {
                "key": "notificationService",
                "implementation": "NotificationService.js",
                "depends": [ "$timeout", "DEFAULT_AUTO_DISMISS" ]
            }
        ]
    }
}