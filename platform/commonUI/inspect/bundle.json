{
    "extensions": {
        "types": [
            {
                "key": "infobubble",
                "name": "Info Bubble",
                "glyph": "\u00EA",
                "description": "Static markup for info bubbles",
                "features": [ "creation" ]
            }
        ],
        "views": [
            {
                "templateUrl": "infobubble.html",
                "name": "Info Bubble",
                "type": "infobubble",
                "key": "infobubble"
            }
        ],
        "templates": [
            {
                "key": "info-table",
                "templateUrl": "info-table.html"
            },
            {
                "key": "info-bubble",
                "templateUrl": "info-bubble.html"
            }
        ],
        "containers": [
            {
                "key": "bubble",
                "templateUrl": "bubble.html",
                "attributes": [ "bubbleTitle", "bubbleLayout" ],
                "alias": "bubble"
            }
        ],
        "gestures": [
            {
                "key": "info",
                "implementation": "gestures/InfoGesture.js",
                "depends": [ "infoService" ]
            }
        ],
        "services": [
            {
                "key": "infoService",
                "implementation": "services/InfoService.js",
                "depends": [
                    "$compile",
                    "$document",
                    "$window",
                    "$rootScope"
                ]
            }
        ]
    }
}