{
    "name": "Plot view for telemetry",
    "extensions": {
        "views": [
            {
                "name": "Plot",
                "key": "plot",
                "glyph": "6",
                "templateUrl": "templates/plot.html",
                "needs": [ "telemetry" ],
                "priority": "preferred",
                "delegation": true
            }
        ],
        "directives": [
            {
                "key": "mctChart",
                "implementation": "MCTChart.js",
                "depends": [ "$interval", "$log" ]
            }
        ],
        "controllers": [
            {
                "key": "PlotController",
                "implementation": "PlotController.js",
                "depends": [ "$scope", "telemetryFormatter", "telemetryHandler" ]
            }
        ]
    }
}