{
    "extensions": {
        "types": [
            {
                "key": "example.page",
                "name": "Page",
                "features": [ "creation" ],
                "properties": [
                    {
                        "key": "url",
                        "name": "URL",
                        "control": "textfield",
                        "pattern": "^(ftp|https?)\\:\\/\\/\\w+(\\.\\w+)*(\\:\\d+)?(\\/\\S*)*$",
                        "required": true
                    }
                ]
            }
        ],
        "views": [
            {
                "templateUrl": "iframe.html",
                "name": "Page",
                "type": "example.page",
                "key": "example.page"
            }
        ],
        "controllers": [
            {
                "key": "IFrameController",
                "implementation": "IFrameController.js",
                "depends": ["$sce"]
            }
        ]
    }
}