{
    "extensions": {
        "components": [
            {
                "implementation": "IdentityAggregator.js",
                "type": "aggregator",
                "provides": "identityService",
                "depends": [ "$q" ]
            },
            {
                "type": "decorator",
                "provides": "creationService",
                "implementation": "services/IdentityCreationDecorator.js",
                "depends": [ "identityService" ]
            }
        ],
        "indicators": [
            {
                "implementation": "IdentityIndicator.js",
                "depends": [ "identityService" ]
            }
        ]
    }
}
